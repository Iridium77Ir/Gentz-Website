<h2 class="page-header">All Images</h2>
<div>
  <h3>You cannot add more than one image here, if you press new image again the old one is deleted.</h3>
  <ul>
      <li><a href="/cp/introImages/new">New Image</a></li>
  </ul>
</div>
<div class="image-grid">
  <% images.forEach(image => { %>
    <a href="/cp/introImages/search/<%= image.id %>">
      <img class="image-image" src="<%= image.ImagePath %>">
    </a>
  <% }) %>
</div>